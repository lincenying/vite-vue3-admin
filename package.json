{
  "name": "vite-vue3-admin",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.6.5",
  "author": "lincenying <lincenying@qq.com>",
  "sideEffects": false,
  "scripts": {
    "prepare": "npx simple-git-hooks",
    "serve": "cross-env DEBUG=vite:transform vite",
    "build_test": "vite build --mode test --emptyOutDir",
    "build_pre": "vite build --mode staging --emptyOutDir",
    "build_prod": "vite build --emptyOutDir",
    "start": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "vue-tsc --noEmit",
    "lint:css": "stylelint '**/*.{css,scss}' --fix"
  },
  "dependencies": {
    "@lincy/async-validation": "^1.3.0",
    "@lincy/utils": "^0.6.2",
    "@vueuse/core": "^13.0.0",
    "axios": "^1.8.4",
    "element-plus": "^2.9.7",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.1",
    "throttle-debounce": "^5.0.2",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vuex": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@iconify-json/ant-design": "^1.2.5",
    "@iconify-json/el": "^1.2.1",
    "@iconify-json/ep": "^1.2.2",
    "@iconify-json/fa": "^1.2.1",
    "@iconify-json/mdi": "^1.2.3",
    "@lincy/eslint-config": "^5.5.2",
    "@lincy/stylelint-config": "^2.0.1",
    "@lincy/unocss-base-config": "^2.2.1",
    "@types/node": "^22.13.13",
    "@types/nprogress": "^0.2.3",
    "@types/throttle-debounce": "^5.0.2",
    "@unocss/eslint-plugin": "^65.5.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitejs/plugin-vue-jsx": "^4.1.2",
    "@vue-macros/volar": "^0.30.15",
    "@vue/compiler-sfc": "^3.5.13",
    "cross-env": "^7.0.3",
    "eslint": "^9.23.0",
    "eslint-plugin-format": "^1.0.1",
    "lint-staged": "^15.5.0",
    "sass": "^1.86.0",
    "simple-git-hooks": "^2.12.1",
    "typescript": "^5.8.2",
    "unocss": "^65.5.0",
    "unplugin-auto-import": "^19.1.1",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.4.1",
    "unplugin-vue-macros": "^2.14.5",
    "vite": "^6.2.3",
    "vite-plugin-inspect": "^11.0.0",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-progress": "^0.0.7",
    "vue-tsc": "^2.2.8"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{[jt]s?(x),vue,md}": [
      "eslint --fix"
    ],
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
