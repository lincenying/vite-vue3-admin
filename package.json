{
  "name": "vite-vue3-admin",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.12.4",
  "author": "lincenying <lincenying@qq.com>",
  "sideEffects": false,
  "scripts": {
    "prepare": "npx simple-git-hooks",
    "serve": "cross-env DEBUG=vite:transform vite",
    "build_test": "vite build --mode test --emptyOutDir",
    "build_pre": "vite build --mode staging --emptyOutDir",
    "build_prod": "vite build --emptyOutDir",
    "start": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "vue-tsc --noEmit",
    "lint:css": "stylelint '**/*.{css,scss}' --fix"
  },
  "dependencies": {
    "@lincy/async-validation": "^1.3.0",
    "@lincy/utils": "^0.6.3",
    "@vueuse/core": "^13.4.0",
    "axios": "^1.10.0",
    "element-plus": "^2.10.2",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "throttle-debounce": "^5.0.2",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "vuex": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@iconify-json/ant-design": "^1.2.5",
    "@iconify-json/el": "^1.2.1",
    "@iconify-json/ep": "^1.2.2",
    "@iconify-json/fa": "^1.2.1",
    "@iconify-json/mdi": "^1.2.3",
    "@lincy/eslint-config": "^5.7.0",
    "@lincy/stylelint-config": "^2.2.0",
    "@lincy/unocss-base-config": "^2.3.0",
    "@types/node": "^24.0.4",
    "@types/nprogress": "^0.2.3",
    "@types/throttle-debounce": "^5.0.2",
    "@unocss/eslint-plugin": "^66.3.2",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.0",
    "@vue-macros/volar": "^3.0.0-beta.15",
    "@vue/compiler-sfc": "^3.5.17",
    "cross-env": "^7.0.3",
    "eslint": "^9.29.0",
    "eslint-plugin-format": "^1.0.1",
    "lint-staged": "^16.1.2",
    "sass": "^1.89.2",
    "simple-git-hooks": "^2.13.0",
    "stylelint": "^16.21.0",
    "typescript": "^5.8.3",
    "unocss": "^66.3.2",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-icons": "^22.1.0",
    "unplugin-vue-components": "^28.7.0",
    "unplugin-vue-macros": "^2.14.5",
    "vite": "^7.0.0",
    "vite-plugin-inspect": "^11.2.0",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-progress": "^0.0.7",
    "vue-tsc": "^2.2.10"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{[jt]s?(x),vue,md}": [
      "eslint --fix"
    ],
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
