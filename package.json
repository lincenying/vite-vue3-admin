{
  "name": "vite-vue3-admin",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.6.2",
  "author": "lincenying <lincenying@qq.com>",
  "sideEffects": false,
  "scripts": {
    "prepare": "npx simple-git-hooks",
    "serve": "cross-env DEBUG=vite:transform vite",
    "build_test": "vite build --mode test --emptyOutDir",
    "build_pre": "vite build --mode staging --emptyOutDir",
    "build_prod": "vite build --emptyOutDir",
    "start": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "vue-tsc --noEmit",
    "lint:css": "stylelint '**/*.{css,scss}' --fix"
  },
  "dependencies": {
    "@lincy/async-validation": "catalog:",
    "@lincy/utils": "catalog:",
    "@vueuse/core": "catalog:",
    "axios": "catalog:",
    "element-plus": "catalog:",
    "mockjs": "catalog:",
    "nprogress": "catalog:",
    "pinia": "catalog:",
    "throttle-debounce": "catalog:",
    "vue": "catalog:",
    "vue-router": "catalog:",
    "vuex": "catalog:",
    "xlsx": "catalog:"
  },
  "devDependencies": {
    "@iconify-json/ant-design": "catalog:",
    "@iconify-json/el": "catalog:",
    "@iconify-json/ep": "catalog:",
    "@iconify-json/fa": "catalog:",
    "@iconify-json/mdi": "catalog:",
    "@lincy/eslint-config": "catalog:",
    "@lincy/stylelint-config": "catalog:",
    "@lincy/unocss-base-config": "catalog:",
    "@types/node": "catalog:",
    "@types/nprogress": "catalog:",
    "@types/throttle-debounce": "catalog:",
    "@unocss/eslint-plugin": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "@vitejs/plugin-vue-jsx": "catalog:",
    "@vue-macros/volar": "catalog:",
    "@vue/compiler-sfc": "catalog:",
    "cross-env": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-format": "catalog:",
    "lint-staged": "catalog:",
    "sass": "catalog:",
    "simple-git-hooks": "catalog:",
    "typescript": "catalog:",
    "unocss": "catalog:",
    "unplugin-auto-import": "catalog:",
    "unplugin-icons": "catalog:",
    "unplugin-vue-components": "catalog:",
    "unplugin-vue-macros": "catalog:",
    "vite": "catalog:",
    "vite-plugin-inspect": "catalog:",
    "vite-plugin-mock": "catalog:",
    "vite-plugin-progress": "catalog:",
    "vue-tsc": "catalog:"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{[jt]s?(x),vue,md}": [
      "eslint --fix"
    ],
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
