{
  "name": "vite-vue3-admin",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.17.1",
  "author": "lincenying <lincenying@qq.com>",
  "sideEffects": false,
  "scripts": {
    "prepare": "npx simple-git-hooks",
    "serve": "cross-env DEBUG=vite:transform vite",
    "build_test": "vite build --mode test --emptyOutDir",
    "build_pre": "vite build --mode staging --emptyOutDir",
    "build_prod": "vite build --emptyOutDir",
    "start": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "vue-tsc --noEmit",
    "lint:css": "stylelint '**/*.{css,scss}' --fix",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@lincy/async-validation": "^1.3.1",
    "@lincy/utils": "^0.7.0",
    "@vueuse/core": "^13.9.0",
    "axios": "^1.12.2",
    "element-plus": "^2.11.4",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "throttle-debounce": "^5.0.2",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1",
    "vuex": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@iconify-json/ant-design": "^1.2.5",
    "@iconify-json/el": "^1.2.2",
    "@iconify-json/ep": "^1.2.3",
    "@iconify-json/fa": "^1.2.2",
    "@iconify-json/mdi": "^1.2.3",
    "@lincy/eslint-config": "^5.9.0",
    "@lincy/stylelint-config": "^2.2.0",
    "@lincy/unocss-base-config": "^2.3.0",
    "@types/node": "^24.6.2",
    "@types/nprogress": "^0.2.3",
    "@types/throttle-debounce": "^5.0.2",
    "@unocss/eslint-plugin": "^66.5.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue-macros/volar": "^3.1.1",
    "@vue/compiler-sfc": "^3.5.22",
    "cross-env": "^10.1.0",
    "eslint": "^9.36.0",
    "eslint-plugin-format": "^1.0.2",
    "lint-staged": "^16.2.3",
    "sass": "^1.93.2",
    "simple-git-hooks": "^2.13.1",
    "stylelint": "^16.24.0",
    "typescript": "^5.9.3",
    "unocss": "^66.5.2",
    "unplugin-auto-import": "^20.2.0",
    "unplugin-icons": "^22.4.2",
    "unplugin-vue-components": "^29.1.0",
    "vite": "^7.1.8",
    "vite-plugin-inspect": "^11.3.3",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-progress": "^0.0.7",
    "vue-component-type-helpers": "^3.1.0",
    "vue-macros": "^3.1.1",
    "vue-tsc": "^3.1.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm run lint-staged"
  },
  "lint-staged": {
    "*.{[jt]s?(x),vue,md}": [
      "eslint --fix"
    ],
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
