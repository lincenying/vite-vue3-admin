{
  "name": "vite-vue3-admin",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.6.1",
  "author": "lincenying <lincenying@qq.com>",
  "sideEffects": false,
  "scripts": {
    "prepare": "npx simple-git-hooks",
    "serve": "cross-env DEBUG=vite:transform vite",
    "build": "vite build --mode=production",
    "build:stag": "vite build --mode=staging",
    "start": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "vue-tsc --noEmit",
    "lint:css": "stylelint '**/*.{css,scss}' --fix"
  },
  "dependencies": {
    "@lincy/utils": "^0.5.5",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.6.8",
    "element-plus": "^2.6.1",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "throttle-debounce": "^5.0.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vuex": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@iconify-json/ant-design": "^1.1.15",
    "@iconify-json/el": "^1.1.8",
    "@iconify-json/ep": "^1.1.15",
    "@iconify-json/fa": "^1.1.8",
    "@iconify-json/mdi": "^1.1.64",
    "@lincy/base-config": "^1.1.0",
    "@lincy/eslint-config": "^4.2.6",
    "@lincy/stylelint-config": "^1.17.4",
    "@lincy/vite-plugin-mock": "^3.0.0",
    "@types/node": "^20.11.30",
    "@types/nprogress": "^0.2.3",
    "@types/throttle-debounce": "^5.0.2",
    "@unocss/eslint-plugin": "^0.58.6",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue-macros/volar": "^0.18.13",
    "@vue/compiler-sfc": "^3.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-format": "^0.1.0",
    "lint-staged": "^15.2.2",
    "sass": "^1.72.0",
    "simple-git-hooks": "^2.11.0",
    "typescript": "^5.4.3",
    "unocss": "^0.58.6",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-icons": "^0.18.5",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-macros": "^2.7.11",
    "vite": "^5.2.2",
    "vite-plugin-inspect": "^0.8.3",
    "vite-plugin-progress": "^0.0.7",
    "vue-tsc": "^2.0.7"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{[jt]s?(x),vue,md}": [
      "eslint --fix"
    ],
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
