{
  "name": "vite-vue3-admin",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.14.2",
  "author": "lincenying <lincenying@qq.com>",
  "sideEffects": false,
  "scripts": {
    "prepare": "npx simple-git-hooks",
    "serve": "cross-env DEBUG=vite:transform vite",
    "build": "vite build --mode=production",
    "build:stag": "vite build --mode=staging",
    "start": "vite preview",
    "lint": "eslint . --flag unstable_ts_config",
    "lint:fix": "eslint . --flag unstable_ts_config --fix",
    "lint:ts": "vue-tsc --noEmit",
    "lint:css": "stylelint '**/*.{css,scss}' --fix"
  },
  "dependencies": {
    "@lincy/async-validation": "^1.2.0",
    "@lincy/utils": "^0.6.1",
    "@vueuse/core": "^11.3.0",
    "axios": "^1.7.7",
    "element-plus": "^2.8.8",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.2.6",
    "throttle-debounce": "^5.0.2",
    "vue": "^3.5.13",
    "vue-router": "^4.4.5",
    "vuex": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@iconify-json/ant-design": "^1.2.2",
    "@iconify-json/el": "^1.2.0",
    "@iconify-json/ep": "^1.2.1",
    "@iconify-json/fa": "^1.2.0",
    "@iconify-json/mdi": "^1.2.1",
    "@lincy/base-config": "^1.3.3",
    "@lincy/eslint-config": "^5.3.0",
    "@lincy/stylelint-config": "^2.0.1",
    "@lincy/vite-plugin-mock": "^3.0.0",
    "@types/node": "^22.9.3",
    "@types/nprogress": "^0.2.3",
    "@types/throttle-debounce": "^5.0.2",
    "@unocss/eslint-plugin": "^0.64.1",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vitejs/plugin-vue-jsx": "^4.1.0",
    "@vue-macros/volar": "^0.30.6",
    "@vue/compiler-sfc": "^3.5.13",
    "cross-env": "^7.0.3",
    "eslint": "^9.15.0",
    "eslint-plugin-format": "^0.1.2",
    "lint-staged": "^15.2.10",
    "sass": "^1.81.0",
    "simple-git-hooks": "^2.11.1",
    "typescript": "^5.6.3",
    "unocss": "^0.64.1",
    "unplugin-auto-import": "^0.18.5",
    "unplugin-icons": "^0.20.1",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-macros": "^2.13.4",
    "vite": "^5.4.11",
    "vite-plugin-inspect": "^0.8.8",
    "vite-plugin-progress": "^0.0.7",
    "vue-tsc": "^2.1.10"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{[jt]s?(x),vue,md}": [
      "eslint --flag unstable_ts_config --fix"
    ],
    "*.{css,scss}": "stylelint --fix --allow-empty-input"
  }
}
